{
  "name": "backend",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "node .dist/index.js",
    "dev": "npx tsx --watch src/index.ts",
    "clean": "rm -rf node_modules .dist",

    "build-docker": "docker build -t express-wol-backend -f Dockerfile .",
    "start-docker": "docker run --env-file ./.env express-wol-backend"
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "express": "^5.1.0",
    "express-session": "^1.18.2",
    "memory-level": "^3.1.0",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.20.6"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^5.0.3",
    "@types/express-session": "^1.18.2",
    "@types/node": "^24.7.0",
    "typescript": "^5.9.3"
  }
}
